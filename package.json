{
  "name": "webpack-react-redux-starter",
  "version": "0.4.0",
  "description": "Modern starter kit for React projects.",
  "license": "MIT",
  "scripts": {
    "serve:src": "cross-env NODE_ENV=development babel-node ./tools/devServer.js",
    "serve:dist": "cross-env NODE_ENV=production babel-node ./tools/prodServer.js",
    "predev": "babel-node ./tools/startMessage.js",
    "dev": "run-p serve:src test:watch",
    "clean": "rimraf ./dist",
    "prebuild": "run-s clean test",
    "build": "cross-env NODE_ENV=production babel-node ./tools/build.js",
    "start": "npm run serve:dist",
    "lint": "eslint ./src ./tools ./config \"./*.js\"",
    "lint:fix": "eslint --fix ./src ./tools ./config \"./*.js\"",
    "test": "cross-env TEST=true mocha-webpack",
    "test:watch": "npm run test --watch",
    "cover": "cross-env COVER=true nyc --reporter=lcov --reporter=text npm run test",
    "list-browsers": "babel-node ./tools/printBrowsersList",
    "precommit": "lint-staged",
    "postcommit": "git update-index -g",
    "heroku-postbuild": "yarn build"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "engines": {
    "yarn": "^1.7.0"
  },
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "@material-ui/lab": "^1.0.0-alpha.11",
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.6",
    "connected-react-router": "4.3.0",
    "history": "4.7.2",
    "lodash": "^4.17.10",
    "prop-types": "15.6.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-easy-crop": "^1.3.0",
    "react-hot-loader": "4.3.3",
    "react-infinite-scroller": "^1.2.0",
    "react-photo-gallery": "^6.2.0",
    "react-redux": "5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "4.3.1",
    "react-select": "^2.0.0",
    "react-smooth-dnd": "^0.5.2",
    "react-virtualized": "^9.20.1",
    "recompose": "0.27.1",
    "redux": "4.0.0",
    "redux-api-middleware": "^2.3.0",
    "redux-saga": "0.16.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "8.6.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "browser-sync": "2.24.4",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-enzyme": "1.0.0-beta.1",
    "chai-fetch-mock": "^2.0.0",
    "chalk": "2.4.1",
    "cheerio": "1.0.0-rc.2",
    "compression": "1.7.2",
    "compression-webpack-plugin": "1.1.11",
    "cross-env": "5.2.0",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.10.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.9.1",
    "fetch-mock": "^6.5.1",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "imagemin-webpack-plugin": "2.1.5",
    "istanbul-instrumenter-loader": "3.0.1",
    "jsdom": "11.11.0",
    "lint-staged": "7.2.0",
    "lost": "8.3.0",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "5.2.0",
    "mocha-webpack": "2.0.0-beta.0",
    "npm-run-all": "4.1.3",
    "nyc": "12.0.2",
    "optimize-css-assets-webpack-plugin": "4.0.2",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.5",
    "react-test-renderer": "16.4.1",
    "redux-devtools-extension": "2.13.2",
    "redux-mock-store": "^1.5.3",
    "rimraf": "2.6.2",
    "sinon": "6.0.0",
    "sinon-chai": "3.2.0",
    "style-loader": "0.21.0",
    "stylelint": "9.2.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-webpack-plugin": "0.10.5",
    "svg-sprite-loader": "3.8.0",
    "svgo": "1.0.5",
    "svgo-loader": "2.1.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
    "webpack-bundle-analyzer": "2.13.1",
